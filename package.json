{
  "author": "David Faure <dfaure@outlook.com>",
  "name": "node-api",
  "description": "complete express server",
  "version": "0.0.1",
  "scripts": {
    "tsnode": "nodemon -I --exec ./node_modules/.bin/ts-node --project ./src-api -- ./src-api/server.ts",
    "start":"yarn tsnode",
    "start-pm2": "mkdir -p ../logs && ./node_modules/.bin/pm2 start --interpreter ./node_modules/.bin/ts-node --project ./src-api -- ./src-api/server.ts -l ../logs/node-out.log",
    "stop-pm2": "./node_modules/.bin/pm2 stop server",
    "logs": "tail -f -n 100 ../logs/node-out*.log",
    "restart": "yarn stop && yarn start && yarn logs",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "redirect-port": "sudo iptables -t nat -I OUTPUT -p tcp -d 127.0.0.1 --dport 80 -j REDIRECT --to-ports 3000 && sudo iptables -t nat -I PREROUTING -p tcp --dport 80 -j REDIRECT --to-ports 3000"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "body-parser": "^1.14.2",
    "browserify-fs": "^1.0.0",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "ejs": "^2.5.7",
    "express": "^4.13.3",
    "extend": "^3.0.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.1",
    "q": "^1.4.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "rxjs": "5.5.2",
    "sequelize": "^4.28.6",
    "sequelize-typescript": "^0.6.2",
    "socket.io": "^2.0.4",
    "sw-toolbox": "3.6.0",
    "tslib": "^1.8.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.1.5",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.57",
    "@types/request": "^2.0.9",
    "@types/sequelize": "^4.0.79",
    "ts-node": "^4.0.2",
    "typescript": "^2.6.2"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "9.*"
  },
  "license": "UNLICENSED",
  "private": false
}
